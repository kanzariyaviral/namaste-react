{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:storybook/recommended"
    ],
    "plugins": ["@typescript-eslint/eslint-plugin"],
    "rules": {
      "no-console": "error",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  }